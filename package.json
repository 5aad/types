{
   "name": "saby-types",
   "version": "19.200.0",
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/Types.git"
   },
   "files": [
      "Types/*"
   ],
   "scripts": {
      "build": "npm run build:typescript && npm run build:units && npm run build:compile",
      "build:typescript": "saby-typescript --install --tsconfig=tsconfig.base.json",
      "build:units": "saby-units --install",
      "build:compile": "tsc -b --verbose",
      "start": "node node_modules/saby-units/cli/server",
      "test": "saby-units %NODE_DEBUG_OPTION% --isolated",
      "test:node": "saby-units --isolated --report",
      "test:node-coverage": "saby-units --isolated --report --coverage",
      "test:browser": "saby-units --browser --report",
      "test:browser-coverage": "saby-units --browser --report --coverage"
   },
   "saby-units": {
      "moduleType": "amd",
      "tests": "test",
      "links": {
         "node_modules/saby-typescript/tslib.js": "tslib.js"
      },
      "initializer": "init-testing.js",
      "url": {
         "port": 1025
      }
   },
   "nyc": {
      "include": [
         "Types/**/*.js"
      ],
      "reporter": [
         "text",
         "html"
      ],
      "cache": false,
      "eager": true,
      "report-dir": "./artifacts/coverage"
   },
   "dependencies": {},
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.7",
      "eslint": "^5.13.0",
      "saby-typescript": "git+https://platform-git.sbis.ru/saby/TypeScript.git#rc-19.200",
      "saby-units": "git+https://platform-git.sbis.ru/saby/Units.git#rc-19.200"
   }
}
