{
   "name": "saby-types",
   "version": "3.18.700",
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/Types.git"
   },
   "files": [
      "Types/*",
      "tsconfig.json"
   ],
   "scripts": {
      "postinstall": "saby-typescript --install --tsconfig=tsconfig.base.json && tsc",
      "prepare": "node prepare",
      "compile": "tsc",
      "start": "node app",
      "test": "node test --isolated",
      "test:debug": "node node_modules/ws-unit-testing/mocha %NODE_DEBUG_OPTION% --amd test-isolated",
      "test:esm-debug": "node node_modules/mocha/bin/mocha %NODE_DEBUG_OPTION% --compilers es:babel-core/register testTypes/**/*.test.*",
      "coverage": "node test --isolated --coverage"
   },
   "config": {
      "app_port": 1024,
      "moduleType": "amd",
      "root": ".",
      "tests": "test",
      "test_server_port": 1025,
      "test_report": "artifacts/xunit-report.xml",
      "test_url": {
         "scheme": "http",
         "host": "localhost",
         "port": 1025,
         "path": "",
         "query": "reporter=XUnit"
      },
      "html_coverage_report": "/artifacts/coverage/index.html",
      "json_coverage_report": "artifacts/coverage.json"
   },
   "nyc": {
      "include": [
         "Types/**/*.js"
      ],
      "reporter": [
         "text",
         "html"
      ],
      "cache": false,
      "eager": true,
      "report-dir": "./artifacts/coverage"
   },
   "dependencies": {
      "saby-typescript": "git+https://platform-git.sbis.ru/saby/TypeScript.git#rc-3.19.100"
   },
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.7",
      "connect": "3.6.x",
      "eslint": "^5.3.0",
      "serve-static": "1.13.x",
      "sbis3-ws": "git+https://git.sbis.ru/sbis/ws.git#rc-3.19.100",
      "ws-unit-testing": "git+https://git.sbis.ru/ws/unit-testing.git#rc-3.18.700",
      "ws-data": "git+https://git.sbis.ru/ws/data.git#rc-3.19.100"
   }
}
