{
   "name": "saby-types",
   "version": "19.100.0",
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/Types.git"
   },
   "files": [
      "Types/*",
      "tsconfig.json"
   ],
   "scripts": {
      "prepare": "saby-typescript --install --tsconfig=tsconfig.base.json",
      "build": "saby-units --install && tsc -b --verbose",
      "start": "node node_modules/saby-units/cli/server",
      "test": "saby-units --isolated",
      "test:debug": "saby-units %NODE_DEBUG_OPTION% --isolated",
      "test:node": "saby-units --isolated --report",
      "test:node-coverage": "saby-units --isolated --report --coverage",
      "test:browser": "saby-units --browser --report",
      "test:browser-coverage": "saby-units --browser --report --coverage"
   },
   "saby-units": {
      "moduleType": "amd",
      "tests": "test",
      "links": {
         "node_modules/sbis3-ws/ws": "WS.Core",
         "node_modules/sbis3-ws/View": "View"
      },
      "initializer": "init-testing.js",
      "url": {
         "port": 1025
      }
   },
   "nyc": {
      "include": [
         "Types/**/*.js"
      ],
      "reporter": [
         "text",
         "html"
      ],
      "cache": false,
      "eager": true,
      "report-dir": "./artifacts/coverage"
   },
   "dependencies": {
   },
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.7",
      "eslint": "^5.3.0",
      "sbis3-ws": "git+https://git.sbis.ru/sbis/ws.git#rc-19.100",
      "saby-typescript": "git+https://platform-git.sbis.ru/saby/TypeScript.git#rc-19.100",
      "saby-units": "git+https://platform-git.sbis.ru/saby/Units.git#rc-19.100"
   }
}
