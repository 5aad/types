{
   "name": "saby-types",
   "version": "20.4000.0",
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/Types.git"
   },
   "license": "MIT",
   "files": [
      "index.ts",
      "Types/*"
   ],
   "scripts": {
      "build": "npm run build:typescript && npm run build:app",
      "build:typescript": "saby-typescript --install --tslint=tslint.json",
      "build:compile": "saby-typescript --compiler 2>&1 || echo done",
      "build:app": "wasaby-cli --tasks=build",
      "start": "wasaby-cli --tasks=startTest --server",
      "test": "wasaby-cli --tasks=startTest --node --report=console",
      "test:coverage": "wasaby-cli --tasks=startTest --node --coverage",
      "lint": "saby-typescript --lint -q Types/**/*.ts",
      "update-cli-store": "wasaby-cli --tasks=initStore",
      "prepare": "npm run update-cli-store"
   },
   "devDependencies": {
      "wasaby-cli": "git+https://github.com/saby/wasaby-cli.git#rc-20.4000",
      "saby-typescript": "git+https://github.com/saby/TypeScript.git#rc-20.4000"
   }
}
