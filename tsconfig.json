{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "importHelpers": true,
    "lib": ["dom", "es5", "es2016", "es2018.promise", "scripthost"],
    "module": "amd",
    "moduleResolution": "classic",
    "noUnusedLocals": true,
    "paths": {
      "Env/*": ["node_modules/wasaby-cli/store/_repos/rmi/src/client/Env/*"],
      "Browser/*": ["node_modules/wasaby-cli/store/_repos/rmi/src/client/Browser/*"],
      "I18n/*": ["node_modules/wasaby-cli/store/_repos/saby-i18n/I18n/*"],
      "WS.Core/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/*"],
      "View/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/View/*"],
      "Inferno/*": ["node_modules/wasaby-cli/store/_repos/saby-inferno/Inferno/*"],
      "UI/*": ["node_modules/wasaby-cli/store/_repos/saby-ui/UI/*"],
      "Vdom/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/Vdom/*"],
      "SbisEnv/*": ["node_modules/wasaby-cli/store/_repos/rmi/src/client/SbisEnv/*"],
      "Application/*": ["node_modules/wasaby-cli/store/_repos/wasaby-app/src/Application/*"],
      "Core/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/core/*"],
      "Lib/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/lib/*"],
      "Transport/*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/transport/*"],
      "json!*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/global"],
      "tmpl!*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/global"],
      "wml!*": ["node_modules/wasaby-cli/store/_repos/sbis3-ws/WS.Core/global"]
    },
    "target": "es5",
    "types" : ["mocha", "requirejs"]
  },
  "exclude": [
      "build-ui",
      "application"
  ]
}
